{
  "name": "TSON Website",
  "version": "1.0.0",
  "main": "app/app.js",
  "dependencies": {
    "bootstrap": "~3.0.0",
    "angular": "~1.2.16",
    "angular-route": "~1.2.16",
    "angulartics": "~0.15.18"
  },
  "overrides": {
    "angular": {
      "main": {
        "normal": [ "angular.js" ],
        "minified": null
      }
    },
    "angular-route": {
      "main": {
        "normal": [ "angular-route.js" ],
        "minified": null
      }
    },
    "angulartics": {
      "main": { 
        "normal": [
          "src/angulartics.js",
          "src/angulartics-ga.js"
        ],
        "minified": [
          "dist/angulartics.min.js",
          "dist/angulartics-ga.min.js"
        ]
      }
    },
    "bootstrap": {
      "ignore": true
    },
    "jquery": {
      "ignore": true
    },
    "jquery-waypoints": {
      "ignore": true
    }
  }
}
